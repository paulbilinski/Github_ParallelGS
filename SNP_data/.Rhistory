theta.prime = runif(n=1,min=theta.current-tuning.parameter/2,max=theta.current+tuning.parameter/2)
if(theta.prime > 1) {
theta.prime = 2-theta.prime
}
if(theta.prime < 0) {
theta.prime = abs(theta.prime)
}
return(theta.prime)
}
